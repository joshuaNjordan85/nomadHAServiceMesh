{
        "server": true,
        "node_name": "consul-server-0",
        "datacenter": "dc1",
        "data_dir": "/home/<yourUser>/consul.d/data",
        "bind_addr": "0.0.0.0",
        "client_addr": "0.0.0.0",
        "advertise_addr": "$SERVER_1",
        "bootstrap_expect": 3,
        "retry_join": ["$SERVER_1", "$SERVER_2", "$SERVER_3"],

        "ui": true,
        "log_level": "DEBUG",
        "enable_syslog": true,
        "acl_enforce_version_8": false
}
